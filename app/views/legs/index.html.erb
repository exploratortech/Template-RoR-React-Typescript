<nav>
  <a id="navbar_logo">Gym Tracker - Legs Day Log</a>
</nav>
<br></br>
  <table>
    <tr>
      <th>Seated Leg Curl</th>
      <th>Leg Extension</th>
      <th>Seated Leg Press</th>
      <th>Hip Abductor Out</th>
      <th>Hip Abductor In</th>
      <th>Hip Thrusts</th>
      <th>Bulgarian Split Squats</th>
    </tr>
    <% @legs.each do |leg| %>
      <tr>
        <td><%= leg.seated_leg_curl %> lb</td>
        <td><%= leg.leg_extension %> lb</td>
        <td><%= leg.seated_leg_press %> lb</td>
        <td><%= leg.hip_abductor_out %> lb</td>
        <td><%= leg.hip_abductor_in %> lb</td>
        <td><%= leg.hip_thrusts %> lb</td>
        <td><%= leg.bulgarian_split_squats %> lb</td>
        <td><%= link_to "Edit", edit_leg_path(leg) %></td>
        <td><%= link_to "Delete", leg_path(leg), method: :delete, data: { confirm: 'Are you certain you want to delete this?' } %></td>
      </tr>
    <% end %>
  </table>
<br></br>
<%= link_to "Create a New Legs Day Log", new_leg_path, class: "button" %>
<br></br>
<%= link_to "Return to Home Page", "/", class: "button" %>